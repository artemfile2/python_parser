CREATE OR REPLACE PROCEDURE SYSTEM.CREATE_NT (period VARCHAR2)
AS
BEGIN
  EXECUTE IMMEDIATE
    'CREATE TABLE SYSTEM.NT05S50'||period||' (
      IDSERV VARCHAR2(36 BYTE),
      IDNOMK VARCHAR2(36 BYTE),
      HKOD   VARCHAR2(25 BYTE),
      NAME_O VARCHAR2(250 BYTE),
      SMO    VARCHAR2(5 BYTE),
      GLPU   VARCHAR2(6 BYTE),
      MCOD   VARCHAR2(6 BYTE),
      KSGH   VARCHAR2(36 BYTE)
    )';
END;
/