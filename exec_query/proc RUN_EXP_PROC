CREATE OR REPLACE PROCEDURE SYSTEM.RUN_EXP_PROC (period VARCHAR2, s_glpu VARCHAR2)
AS
BEGIN

  EXP_ADRES(period, s_glpu);

  EXP_DATEUSL(period, s_glpu);

  EXP_DOCTOR(period, s_glpu);

  EXP_USL_AGE(period, s_glpu);

  EXP_DIAG_ERR(period, s_glpu);

  EXP_DB_ERR(period, s_glpu);

  TO_XLS(period, s_glpu);

END;
/