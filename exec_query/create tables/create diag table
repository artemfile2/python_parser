CREATE TABLE XML_DIAG (
  IDSERV    VARCHAR2(38 BYTE),
  ID        VARCHAR2(38 BYTE),
  GLPU      VARCHAR2(6 BYTE),
  PERIOD    VARCHAR2(12 BYTE),
  DIAG_TIP  NUMBER,
  DIAG_CODE NUMBER,
  DIAG_RSLT NUMBER
)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;

CREATE INDEX IDX_XML_DIAG_GLPU ON XML_DIAG (GLPU)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;

CREATE INDEX IDX_XML_DIAG_IDSERV ON XML_DIAG (IDSERV)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;

CREATE INDEX IDX_XML_DIAG_PERIOD ON XML_DIAG (PERIOD)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;