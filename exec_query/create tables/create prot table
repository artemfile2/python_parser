CREATE TABLE XML_PROT (
  IDSERV VARCHAR2(38 BYTE),
  ID     VARCHAR2(38 BYTE),
  GLPU   VARCHAR2(6 BYTE),
  PERIOD VARCHAR2(12 BYTE),
  PROT   NUMBER,
  D_PROT DATE
)
TABLESPACE USERS
LOGGING;

CREATE INDEX IDX_XML_PROT_GLPU ON XML_PROT (GLPU)
TABLESPACE USERS
LOGGING;

CREATE INDEX IDX_XML_PROT_IDSERV ON XML_PROT (IDSERV)
TABLESPACE USERS
LOGGING;

CREATE INDEX IDX_XML_PROT_PERIOD ON XML_PROT (PERIOD)
TABLESPACE USERS
LOGGING;