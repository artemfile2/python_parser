CREATE TABLE XML_ONK (
  IDSERV    VARCHAR2(38 BYTE),
  ID        VARCHAR2(38 BYTE),
  GLPU      VARCHAR2(6 BYTE),
  PERIOD    VARCHAR2(12 BYTE),
  DS1_T     NUMBER,
  STAD      NUMBER,
  ONK_T     NUMBER,
  ONK_N     NUMBER,
  ONK_M     NUMBER,
  MTSTZ     NUMBER,
  SOD       FLOAT(4),
  PR_CONS   NUMBER,
  USL_TIP   NUMBER,
  HIR_TIP   NUMBER,
  LEK_TIP_L NUMBER,
  LEK_TIP_V NUMBER,
  LUCH_TIP  NUMBER,
  DT_CONS   DATE
)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;

CREATE INDEX IDX_XML_ONK_GLPU ON XML_ONK (GLPU)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;

CREATE INDEX IDX_XML_ONK_IDSERV ON XML_ONK (IDSERV)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;

CREATE INDEX IDX_XML_ONK_PERIOD ON XML_ONK (PERIOD)
TABLESPACE USERS
STORAGE (INITIAL 64 K
         MAXEXTENTS UNLIMITED)
LOGGING;